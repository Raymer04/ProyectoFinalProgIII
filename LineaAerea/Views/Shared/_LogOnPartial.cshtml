@if(Session["usuario"]!=null) { 
    <text>Welcome <strong>@Context.User.Identity.Name</strong>!
    @Html.ActionLink("Salir", "Salir", "Usuarios", null, new { @class = "button" })</text>
}
else {
    @:@Html.ActionLink("Iniciar Sesión", "IniciarSesion", "Usuarios", null, new { @class = "button" })
}
